{
  "base": {
    "configReloadTimeSeconds": 900,
    "genericDevices": [
      {
        "identifier": "television",
        "checks": ["ping"],
        "pingIntervalMs": null,
        "hostAddress": "192.168.0.XXX",
        "name": "Living Room Television",
        "onAvailable": [
          {
            "targetSpeakers": ["play3", "play5"],
            "sourceSpeakers": ["play3", "play5"],
            "pausePlay": true,
            "setVolume": "lowestSource"
          },
          {
            "targetSpeakers": ["beam"],
            "setVolume": 10,
            "leaveGroup": true
          }
        ],
        "onUnavailable": [
          {
            "targetSpeakers": ["beam", "play3", "play5"],
            "sourceSpeakers": ["play3", "play5"],
            "setVolume": "lowestSource",
            "joinGroup": "play5"
          }
        ]
      }
    ],
    "sonos": {
      "maxSetVolume": 20,
      "speakers": {
        "XX-XX-XX-XX-XX-XX:X": {
          "identifier": "beam"
        },
        "YY-YY-YY-YY-YY-YY:Y": {
          "identifier": "play5"
        },
        "XX-YY-XX-YY-XX-YY:X": {
          "identifier": "play3"
        }
      }
    },
    "ping": {
      "defaultInterval": 1000,
      "timeOutMs": 300,
      "retries": 0
    }
  },
  "development": {
  },
  "production": {
  }
}